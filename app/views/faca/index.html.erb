

        <div class="container">
            <h2>Food and Clothing Assistance Services</h2>

            <div class="table-responsive">
                <table class="table table-striped table-bordered" width="100%">
                    <thead>
                        <tr>
                            <th>Service Name</th>
                            <th>Address</th>
                            <th>City</th>
                            <th>State</th>
                            <th>ZipCode</th>
                            <th>Phone Number</th>
                            <th>Website</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% @services.each do |service| %>
                            <tr>
                              <td><h4><a href="/services/<%= service.id %>" target="_blank"><%= service.name %></a></h4></td>
                              <td><h5> <%= service.address %></h5></td>
                              <td><h5> <%= service.city %></h5></td>
                              <td><h5> <%= service.state %></h5></td>
                              <td><h5> <%= service.zip_code %></h5></td>
                              <td><h5> <%= service.phone_number %></h5></td>
                              <td><h5> <%= service.website %></h5></td>
                          </tr>
                      <% end %>
                  </tbody>
              </table>
          </div>
      </div>

                            
                        
                   
                    
                            
                            
                           
                                   
                                
                       
                    
